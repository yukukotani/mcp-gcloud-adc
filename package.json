{
  "name": "typescript-template",
  "version": "1.0.0",
  "type": "module",
  "description": "TypeScript template for Coding AI Agent",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "biome check --write .",
    "lint:ci": "biome ci .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@power-assert/runtime": "^0.3.0",
    "@types/node": "^24.0.15",
    "@vitest/coverage-v8": "^3.2.4",
    "rollup-plugin-power-assert": "^0.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}